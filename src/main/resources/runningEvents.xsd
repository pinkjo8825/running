                <xs:element name="runningEvent" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="runningEventName" type="xs:string"/>
                            <xs:element name="district" type="xs:string"/>
                            <xs:element name="raceTypes">
                                <xs:complexType>
